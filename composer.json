{
    "name": "svensp/callection",
    "description": "Track 'current' items for stacked closure calls",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Speckmaier",
            "email": "sven@speckmaier.de"
        }
    ],
    "autoload": {
      "psr-4": {
        "Callection\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "CallectionTests\\": "tests/"
      }
    },
    "require": {
    },
    "scripts": {
      "codestyle": "vendor/bin/phpcs --standard=PSR2 --extensions=php src && vendor/bin/phpcs --standard=tests/.codestyle.xml --extensions=php tests",
      "fixstyle": "vendor/bin/phpcbf --standard=PSR2 --extensions=php src ; vendor/bin/phpcbf --standard=tests/.codestyle.xml --extensions=php tests"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
